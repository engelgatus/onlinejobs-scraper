# Environment variables for local development
# Copy this file to .env and fill in your actual values

# Discord webhook URL for your Command Center server
# Get this from: Discord Server Settings > Integrations > Webhooks > Create Webhook
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Optional: Override default database path
# DATABASE_PATH=data/jobs.db

# Optional: Override default keywords (comma-separated)
# KEYWORDS=automation,entry level,associate,admin,operations

# Optional: Override default days back for scraping
# DEFAULT_DAYS_BACK=5
